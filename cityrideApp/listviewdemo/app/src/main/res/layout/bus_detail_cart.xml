<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginHorizontal="@dimen/dim_10"
    android:layout_marginTop="@dimen/dim_10"
    android:layout_marginBottom="@dimen/dim_20"
    android:padding="@dimen/dim_5"
    android:scrollbars="vertical"
    app:cardCornerRadius="@dimen/dim_20">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintEnd_toStartOf="@+id/textView3"
        app:layout_constraintStart_toStartOf="@+id/textView3">

        <TextView
            android:id="@+id/textid"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/dim_16"
            android:layout_marginTop="@dimen/dim_16"
            android:text="Bus ID : "
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/busid"
            android:layout_width="@dimen/dim_69"
            android:layout_height="0dp"
            android:layout_marginStart="@dimen/dim_2"
            android:layout_marginTop="@dimen/dim_16"
            android:text="TextView"
            android:textAlignment="viewStart"
            app:layout_constraintStart_toEndOf="@+id/textid"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textstart"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/dim_16"
            android:layout_marginTop="@dimen/dim_16"
            android:text="Starting Point"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textid" />

        <TextView
            android:id="@+id/startdes"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/dim_16"
            android:layout_marginTop="@dimen/dim_16"
            android:layout_marginEnd="@dimen/dim_16"
            android:text="TextView"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/textstart"
            app:layout_constraintTop_toBottomOf="@+id/txtp" />

        <TextView
            android:id="@+id/textend"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/dim_16"
            android:layout_marginTop="@dimen/dim_16"
            android:layout_marginBottom="@dimen/dim_16"
            android:text="Ending Point"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textstart" />

        <TextView
            android:id="@+id/desend"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/dim_16"
            android:layout_marginEnd="@dimen/dim_16"
            android:layout_marginBottom="@dimen/dim_16"
            android:text="TextView"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/textend"
            app:layout_constraintTop_toBottomOf="@+id/startdes"
            app:layout_constraintVertical_bias="1.0" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:layout_marginStart="@dimen/dim_92"
            android:layout_marginTop="@dimen/dim_16"
            android:layout_marginBottom="@dimen/dim_15"
            android:text="Ticket Price :"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/startdes"
            app:layout_constraintStart_toEndOf="@+id/busid"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/txtp"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:layout_marginStart="@dimen/dim_2"
            android:layout_marginTop="@dimen/dim_16"
            android:layout_marginRight="@dimen/dim_16"
            android:text="TextView"
            android:textAlignment="viewStart"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.058"
            app:layout_constraintStart_toEndOf="@+id/textView3"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.cardview.widget.CardView>
